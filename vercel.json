{
  "version": 2,
  "builds": [
    {
      "src": "redirect-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/(.*)",
      "dest": "/redirect-server.js"
    }
  ],
  "regions": ["sin1"],
  "github": {
    "silent": true,
    "autoAlias": true
  }
}