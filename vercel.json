{
  "version": 2,
  "builds": [
    {
      "src": "redirect-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "bot.js"
    },
    {
      "src": "/(.*)",
      "dest": "redirect-server.js"
    }
  ],
  "regions": ["sin1"],
  "github": {
    "silent": true,
    "autoAlias": true
  },
  "alias": ["midget.pro"]
}